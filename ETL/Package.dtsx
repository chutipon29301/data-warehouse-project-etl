                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
	b.DiscountCode, 
	a.ItemCode,
	a.TransDate,
	SUM(a.Qty) Qty,
	SUM(a.Price) Price,
	SUM(a.PricePromo) PricePromo,
	SUM(a.Cost) Cost,
	Sum(a.SalesAmount) SalesAmount,
	SUM(a.NetSalesAmount) NetSalesAmount,
	SUM(a.NetSalesAmountExcTax) NetSalesAmountExcTax,
	SUM(a.TaxAmount) TaxAmount,
	SUM(a.DiscTotalAmount) DiscTotalAmount,
	SUM(a.DiscLineAmountTotal) DiscLineAmountTotal,
	SUM(a.DiscLineAmountVendor) DiscLineAmountVendor,
	SUM(a.DiscLineAmountCompany) DiscLineAmountCompany,
	SUM(a.DiscLineAmountLoyalty) DiscLineAmountLoyalty,
	SUM(a.DiscBillAmount) DiscBillAmount,
	SUM(a.NetSalesAmount-a.Cost) Profit,
	c.DateKey,
	d.TimeKey,
	e.ItemKey,
	CASE WHEN f.DiscountKey IS NULL THEN 0 ELSE f.DiscountKey END DiscountKey
FROM SALES_TRANSACTION a
LEFT JOIN SALES_DISCOUNT_TRANSACTION b ON a.TransDate = b.TransDate AND a.SalesLine = b.SalesLine AND a.CompanyCode = b.CompanyCode AND a.StoreNumber = b.StoreNumber AND a.ReceiptID = b.ReceiptID AND b.DiscountCode &lt;&gt; ''
LEFT JOIN DPMS_DB_STAR.dbo.DateDim c ON c.TheDate = CAST(a.TransDate AS DATE)
LEFT JOIN DPMS_DB_STAR.dbo.TimeDim d ON d.Hour = DATEPART(HH, a.TransDate) AND d.Minute = DATEPART(MI, a.TransDate) AND d.Second = DATEPART(SS, a.TransDate)
LEFT JOIN DPMS_DB_STAR.dbo.ItemDim e ON e.ItemCode = a.ItemCode
LEFT JOIN DPMS_DB_STAR.dbo.DiscountMasterDim f ON f.DiscountCode = b.DiscountCode
GROUP BY b.DiscountCode, a.ItemCode, a.TransDate, c.DateKey, d.TimeKey, e.ItemKey, f.DiscountKey</property>
