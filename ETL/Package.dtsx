                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
	a.CompanyCode,
	a.StoreNumber,
	a.TransDate,
	a.ReceiptID,
	b.ItemCode,
	SUM(b.Qty) Qty,
	AVG(b.Price) AveragePrice,
	AVG(b.PricePromo) AveragePricePromo,
	SUM(b.Cost) Cost,
	Sum(b.SalesAmount) SalesAmount,
	SUM(b.NetSalesAmount) NetSalesAmount,
	SUM(b.NetSalesAmountExcTax) NetSalesAmountExcTax,
	SUM(b.TaxAmount) TaxAmount,
	SUM(b.DiscTotalAmount) DiscTotalAmount,
	SUM(b.DiscLineAmountTotal) DiscLineAmountTotal,
	SUM(b.DiscLineAmountVendor) DiscLineAmountVendor,
	SUM(b.DiscLineAmountCompany) DiscLineAmountCompany,
	SUM(b.DiscLineAmountLoyalty) DiscLineAmountLoyalty,
	SUM(b.DiscBillAmount) DiscBillAmount,
	SUM(b.NetSalesAmount - b.Cost) Profit,
	d.MemberKey,
	d.MemberDemographicKey,
	a.CardNumber,
	e.CardKey,
	f.ItemKey,
	g.DateKey,
	h.TimeKey,
	i.RetailKey
FROM DPMS_DB.dbo.SALES_TRANSACTION_SUMMARY a
LEFT JOIN DPMS_DB.dbo.SALES_TRANSACTION b ON a.CompanyCode = b.CompanyCode AND a.StoreNumber = b.StoreNumber AND a.TransDate = b.TransDate AND a.ReceiptID = b.ReceiptID
LEFT JOIN DPMS_DB.dbo.MEMBER_PROFILE c ON c.CardNumber = a.CardNumber
LEFT JOIN DPMS_DB_STAR.dbo.MemberDim d ON d.AccountNum = c.AccountNum
LEFT JOIN DPMS_DB_STAR.dbo.CardDim e ON e.CardNumber = a.CardNumber
LEFT JOIN DPMS_DB_STAR.dbo.ItemDim f ON f.ItemCode = b.ItemCode
LEFT JOIN DPMS_DB_STAR.dbo.DateDim g ON g.TheDate = CAST(a.TransDate AS DATE)
LEFT JOIN DPMS_DB_STAR.dbo.TimeDim h ON h.Hour = DATEPART(HH, a.TransDate) AND h.Minute = DATEPART(MI, a.TransDate) AND h.Second = DATEPART(SS, a.TransDate)
LEFT JOIN DPMS_DB_STAR.dbo.RetailDim i ON i.CompanyCode = a.CompanyCode AND i.StoreNumber = a.StoreNumber
WHERE a.RetailType = 1 AND b.ReceiptID IS NOT NULL
GROUP BY a.CompanyCode, a.StoreNumber, a.TransDate, a.ReceiptID, a.CardNumber, b.ItemCode, d.MemberKey, d.MemberDemographicKey, e.CardKey, f.ItemKey, g.DateKey, h.TimeKey, i.RetailKey</property>
