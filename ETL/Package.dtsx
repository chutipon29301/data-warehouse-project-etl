                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	a.ItemKey,
	a.RetailKey,
	c.MonthKey,
	SUM(a.DailyQty) AS MonthlyQty,
	AVG(a.DailyAvgPrice) AS MonthlyAvgPrice,
	AVG(a.DailyAvgPricePromo) AS MonthlyAvgPricePromo,
	SUM(a.DailyCost) AS MonthlyCost,
