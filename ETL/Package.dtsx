                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	SHOP_RETAIL.StoreName, SHOP_RETAIL.StoreCategory, COMPANY_DETAIL.CompanyName, SALES_TRANSACTION_SUMMARY.CompanyCode, SHOP_RETAIL.StoreNumber
FROM
	SHOP_RETAIL  
LEFT JOIN
	SALES_TRANSACTION_SUMMARY ON SALES_TRANSACTION_SUMMARY.StoreNumber = SHOP_RETAIL.StoreNumber
LEFT JOIN
	(SELECT SALES_TRANSACTION.CompanyCode, SALES_TRANSACTION.CompanyName FROM SALES_TRANSACTION GROUP BY SALES_TRANSACTION.CompanyCode, SALES_TRANSACTION.CompanyName) COMPANY_DETAIL
