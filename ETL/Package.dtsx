		WHEN a.Age &gt; 60 AND a.Age &lt;= 70 THEN '60-70'
		WHEN a.Age &gt; 70 AND a.Age &lt;= 80 THEN '70-80'
		WHEN a.Age &gt; 80 THEN '&gt;80'
		ELSE NULL
	END) AgeRange,
	(CASE
		WHEN a.Incomepermonth &lt;= 20000 THEN '0-20000'
		WHEN a.Incomepermonth &gt; 20000 AND a.Incomepermonth &lt;= 50000 THEN '20000-50000'
		WHEN a.Incomepermonth &gt; 50000 AND a.Incomepermonth &lt;= 80000 THEN '50000-80000'
