	SUM(a.DailyDiscBillAmount) AS MonthlyDiscBillAmount,
	SUM(a.DailyProfit) AS MonthlyProfit
FROM
	DPMS_DB_STAR.dbo.DailyRetailItemTransactionFact a
LEFT JOIN DPMS_DB_STAR.dbo.DateDim b ON a.DateKey = b.DateKey
LEFT JOIN DPMS_DB_STAR.dbo.MonthDim c ON c.Month = b.Month AND c.Year = b.Year
