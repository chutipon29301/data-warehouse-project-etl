WHERE SHOP_RETAIL.StoreNumber &lt;&gt; ''
GROUP BY SHOP_RETAIL.StoreName, SHOP_RETAIL.StoreCategory, COMPANY_DETAIL.CompanyName, SALES_TRANSACTION_SUMMARY.CompanyCode, SHOP_RETAIL.StoreNumber</property>
