                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	CASE WHEN a.TenantID = '' THEN NULL ELSE a.TenantID END TenantId,
	a.CardNumber,
	CASE WHEN a.CompanyCode = '' THEN NULL ELSE a.CompanyCode END CompanyCode,
	CASE WHEN a.StoreNumber = '' THEN NULL ELSE a.StoreNumber END StoreNumber,
	a.ReceiptID,
	a.TransDate,
	a.NetSalesAmount,
	a.EarnPoint,
	c.MemberKey,
	c.MemberDemographicKey,
	d.DateKey,
	e.TimeKey,
	f.CardKey,
	g.TenantKey,
	h.RetailKey
FROM
	DPMS_DB.dbo.SALES_TRANSACTION_SUMMARY a
LEFT JOIN DPMS_DB.dbo.MEMBER_PROFILE b ON a.CardNumber = b.CardNumber
LEFT JOIN DPMS_DB_STAR.dbo.MemberDim c ON c.AccountNum = b.AccountNum
LEFT JOIN DPMS_DB_STAR.dbo.DateDim d ON d.TheDate = CAST(a.TransDate AS DATE)
LEFT JOIN DPMS_DB_STAR.dbo.TimeDim e ON e.Hour = DATEPART(HH, a.TransDate) AND e.Minute = DATEPART(MI, a.TransDate) AND e.Second = DATEPART(SS, a.TransDate)
LEFT JOIN DPMS_DB_STAR.dbo.CardDim f ON f.CardNumber = a.CardNumber
LEFT JOIN DPMS_DB_STAR.dbo.TenantDim g ON g.TenantId = a.TenantID
LEFT JOIN DPMS_DB_STAR.dbo.RetailDim h ON h.StoreNumber = a.StoreNumber</property>
