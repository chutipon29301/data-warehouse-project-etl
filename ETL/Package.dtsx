                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	a.CardNumber,
	CASE WHEN a.CompanyCode = '' THEN NULL ELSE a.CompanyCode END CompanyCode,
	CASE WHEN a.StoreNumber = '' THEN NULL ELSE a.StoreNumber END StoreNumber,
	a.ReceiptID,
	a.TransDate,
	a.NetSalesAmount,
	a.EarnPoint,
	c.MemberKey,
	c.MemberDemographicKey,
	d.DateKey,
	e.TimeKey,
	f.CardKey,
	g.RetailKey,
	i.TotalTax,
	i.DiscountTotalAmount,
	i.Profit,
	h.EmployeeKey
FROM
	DPMS_DB.dbo.SALES_TRANSACTION_SUMMARY a
LEFT JOIN DPMS_DB.dbo.MEMBER_PROFILE b ON a.CardNumber = b.CardNumber
LEFT JOIN DPMS_DB_STAR.dbo.MemberDim c ON c.AccountNum = b.AccountNum
LEFT JOIN DPMS_DB_STAR.dbo.DateDim d ON d.TheDate = CAST(a.TransDate AS DATE)
LEFT JOIN DPMS_DB_STAR.dbo.TimeDim e ON e.Hour = DATEPART(HH, a.TransDate) AND e.Minute = DATEPART(MI, a.TransDate) AND e.Second = DATEPART(SS, a.TransDate)
LEFT JOIN DPMS_DB_STAR.dbo.CardDim f ON f.CardNumber = a.CardNumber
LEFT JOIN DPMS_DB_STAR.dbo.RetailDim g ON g.StoreNumber = a.StoreNumber
LEFT JOIN DPMS_DB_STAR.dbo.EmployeeDim h ON h.EmployeeNumber = a.EmployeeNumber
LEFT JOIN (
    SELECT
        j.ReceiptID,
        j.TransDate,
        j.CompanyCode,
        j.StoreNumber,
        SUM(k.TaxAmount) TotalTax,
        SUM(k.DiscTotalAmount) DiscountTotalAmount,
        SUM(k.NetSalesAmount - k.Cost) Profit
    FROM SALES_TRANSACTION_SUMMARY j
    LEFT JOIN SALES_TRANSACTION k ON j.ReceiptID = k.ReceiptID AND j.TransDate = k.TransDate AND j.CompanyCode = k.CompanyCode AND j.StoreNumber = k.StoreNumber
    WHERE j.tenantID = ''
    GROUP BY j.ReceiptID, j.TransDate, j.CompanyCode, j.StoreNumber
) i  ON a.ReceiptID = i.ReceiptID AND a.TransDate = i.TransDate AND a.CompanyCode = i.CompanyCode AND a.StoreNumber = i.StoreNumber
WHERE a.TenantID = ''</property>
