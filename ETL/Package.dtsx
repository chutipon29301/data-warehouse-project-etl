                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	DISTINCT a.AccountNum,
	a.BirthDate,
	a.Incomepermonth,
	a.MaritalStatus,
	a.Occupation,
	CAST(a.Age AS tinyint) Age,
	(CASE
		WHEN a.Age &lt;= 10 THEN '0-10'
		WHEN a.Age &gt; 10 AND a.Age &lt;= 20 THEN '10-20'
		WHEN a.Age &gt; 20 AND a.Age &lt;= 30 THEN '20-30'
		WHEN a.Age &gt; 30 AND a.Age &lt;= 40 THEN '30-40'
		WHEN a.Age &gt; 40 AND a.Age &lt;= 50 THEN '40-50'
