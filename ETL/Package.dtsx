              description="OLE DB Source"
              name="OLE DB Source"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	prodMaster.ItemCode,
	prodMaster.ItemName,
	h6.Name Level6Category,
	h5.Name Level5Category,
	h4.Name Level4Category,
	h3.Name Level3Category,
	h2.Name Level2Category,
	h1.Name Level1Category,
	brand.BrandCode,
	brand.Brand,
	barcode.Barcode,
	vendor.VendorCode,
	vendor.VendorName
FROM PRODUCT_MASTER prodMaster
LEFT JOIN PRODUCT_HIERARCHY h6 ON h6.Category = prodMaster.Category AND h6.Level = 6
LEFT JOIN PRODUCT_HIERARCHY h5 ON (h5.Category = h6.ParentCategory OR h5.Category = prodMaster.Category) AND h5.Level = 5
LEFT JOIN PRODUCT_HIERARCHY h4 ON (h4.Category = h5.ParentCategory OR h4.Category = prodMaster.Category) AND h4.Level = 4
LEFT JOIN PRODUCT_HIERARCHY h3 ON (h3.Category = h4.ParentCategory OR h3.Category = prodMaster.Category) AND h3.Level = 3
LEFT JOIN PRODUCT_HIERARCHY h2 ON (h2.Category = h3.ParentCategory OR h2.Category = prodMaster.Category) AND h2.Level = 2
LEFT JOIN PRODUCT_HIERARCHY h1 ON (h1.Category = h2.ParentCategory OR h1.Category = prodMaster.Category) AND h1.Level = 1
LEFT JOIN (
	SELECT 
		DISTINCT a.ItemCode, 
		(
			SELECT STRING_AGG(b.Barcode,'|') 
			FROM (
				SELECT DISTINCT c.Barcode FROM SALES_TRANSACTION c
				WHERE c.ItemCode = a.ItemCode AND (c.Barcode &lt;&gt; '' OR c.Barcode IS NOT NULL)
			) b
		) Barcode
	FROM SALES_TRANSACTION a) barcode 
	ON barcode.ItemCode = prodMaster.ItemCode
OUTER APPLY (
	SELECT 
		TOP 1 
			a.ItemCode,
			CASE WHEN a.Brand = '' THEN NULL ELSE a.Brand END Brand,
			a.BrandCode,
			COUNT(a.ItemCode) BrandCount
	FROM 
		SALES_TRANSACTION a
	WHERE a.ItemCode = prodMaster.ItemCode AND a.BrandCode &lt;&gt; '' 
	GROUP BY 
		a.Brand, a.BrandCode, a.ItemCode
	ORDER BY 
		BrandCount DESC
) brand
OUTER APPLY(
	SELECT 
		TOP 1 
			a.ItemCode,
			CASE WHEN  a.VendorCode = '' THEN NULL ELSE a.VendorCode END VendorCode,
			a.VendorName,
			COUNT(a.ItemCode) VendorCount
	FROM 
		SALES_TRANSACTION a
	WHERE a.ItemCode = prodMaster.ItemCode AND a.VendorCode &lt;&gt; '' 
	GROUP BY 
		a.ItemCode, a.VendorCode, a.VendorName
	ORDER BY 
		VendorCount DESC
) vendor</property>
