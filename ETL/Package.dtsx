		WHEN a.Incomepermonth &gt; 120000 AND a.Incomepermonth &lt;= 200000 THEN '120000-200000'
		WHEN a.Incomepermonth &gt; 200000 THEN '&gt;200000'
		ELSE NULL
	END) IncomeRange
